import java.util.*;
class Sum
{
public static void main(String args[])
{
Scanner s=new Scanner(System.in);
int n=s.nextInt();
int a[]=new int[n];
for(int i=0;i<n;i++)
{
a[i]=s.nextInt();
}
for(int i=0;i<n;i++)
{
int sum=0;
while(a[i]>0)
{
int m=a[i]%10;
a[i]=a[i]/10;
sum=sum+m;
}
System.out.print(sum+" ");
}
}
}
Output:
5
12 23 432 51 31
3 5 9 6 4
